{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "rootDir": "./",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "skipLibCheck": true
  },
  "include": [
    "*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
